<div class="container" style="margin-top:50px; margin-bottom: 50px;">
  <h1>신청 내역</h1>
  <hr>
  <% @one_post.each do |user| %>
    <div class="form-group row">
      <label class="col-2 col-form-label">건물 번호</label>
      <div class="col-10">
        <p><%= user.building %></p>
      </div>
    </div>
    <div class="form-group row">
      <label class="col-2 col-form-label">강의실</label>
      <div class="col-10">
        <p><%= user.class_num %></p>
      </div>
    </div>
    <div class="form-group row">
      <label class="col-2 col-form-label">이메일</label>
      <div class="col-10">
        <p><%= user.email_id %></p>
      </div>
    </div>
    <div class="form-group row">
      <label for="example-tel-input" class="col-2 col-form-label">휴대폰 번호</label>
      <div class="col-10">
        <p><%= user.phone %></p>
      </div>
    </div>
    <div class="form-group row">
      <label class="col-2 col-form-label">인원수</label>
      <div class="col-10">
        <p><%= user.num_of_people %></p>
      </div>
    </div>
    <div class="form-group row">
      <label for="example-date-input" class="col-2 col-form-label">날짜</label>
      <div class="col-10">
        <p><%= user.date_of_rent %></p>
      </div>
    </div>
    <div class="form-group row">
      <label for="example-time-input" class="col-2 col-form-label">시간</label>
      <div class="col-10">
        <p><%= user.time_of_rent %></p>
      </div>
    </div>
    <a class="btn btn-primary" href="/form/<%= user.id %>" role="button">신청내역 삭제</a>
  <% end %>
</div>
